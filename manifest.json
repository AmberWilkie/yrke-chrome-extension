{
  "name": "Yrke",
  "version": "1.0",
  "description": "Add data to a google spreadsheet",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js",
      "scripts.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png",
      "32": "logo32.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
